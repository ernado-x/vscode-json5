{
    "name": "vscode-jstp",
    "displayName": "JSTP Syntax",
    "description": "Provides syntax support for JSTP in .jstp, .jsrm, .jsrd and .jsrs files.",
    "version": "0.5.0",
    "publisher": "ernado-x",
    "license": "MIT",
    "engines": {
        "vscode": "^1.3.0"
    },
    "galleryBanner": {
        "color": "#1E1E1E",
        "theme": "dark"
    },
    "icon": "icon.png",
    "homepage": "https://github.com/ernado-x/vscode-jstp/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/ernado-x/vscode-jstp"
    },
    "keywords": [
        "jstp",
        "syntax",
        "highlight",
        "metarhia"
    ],
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "jstp",
                "aliases": [
                    "JPT",
                    "jstp"
                ],
                "extensions": [
                    ".jstp",
                    ".jsrm",
                    ".jsrd",
                    ".jsrs"
                ],
                "configuration": "./jstp.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "jstp",
                "scopeName": "source.jstp",
                "path": "./syntaxes/jstp.json"
            }
        ]
    }
}